@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix pset:  <http://lv2plug.in/ns/ext/presets#> .
@prefix state: <http://lv2plug.in/ns/ext/state#> .

<default-preset>
	a pset:Preset ;
	lv2:appliesTo <https://github.com/dcoredump/GenericFluidPlug_[% name %]> ;
	lv2:port [
		lv2:symbol "level" ;
		pset:value 1.0
	] , [
		lv2:symbol "program" ;
		pset:value 0.0
	] .

